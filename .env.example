# aech-cli-visualize Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# LLM Configuration
# =============================================================================

# Model for internal agents (iterate command, data analyzer)
# Format: provider:model (e.g., anthropic:claude-sonnet-4-20250514, openai:gpt-4o)
# Default: anthropic:claude-sonnet-4-20250514
AECH_LLM_WORKER_MODEL=anthropic:claude-sonnet-4-20250514

# Model for VLM validation (visual QA of rendered dashboards)
# Must support vision/images
# Default: anthropic:claude-sonnet-4-20250514
AECH_VLM_MODEL=anthropic:claude-sonnet-4-20250514

# =============================================================================
# API Keys (required for the provider you're using)
# =============================================================================

# Anthropic API key (for anthropic: models)
ANTHROPIC_API_KEY=

# OpenAI API key (for openai: models)
OPENAI_API_KEY=
